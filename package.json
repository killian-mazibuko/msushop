{
  "name": "msushop",
  "version": "1.0.0",
  "type": "module",
  "description": "Ecommerce application built with the MERN stack",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/killian-mazibuko/msushop.git"
  },
  "author": "Killian Mazibuko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/killian-mazibuko/msushop/issues"
  },
  "homepage": "https://github.com/killian-mazibuko/msushop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "mongoose": "^8.2.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0"
  }
}
